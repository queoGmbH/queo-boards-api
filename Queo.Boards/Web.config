      <resource uri="assembly://Queo.Boards/Queo.Boards.Config/Spring.SignalR.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Database.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Persistence.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Infrastructure.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Scheduling.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Services.xml" />
      <resource uri="assembly://Queo.Boards.Core/Queo.Boards.Core.Config/Spring.Validators.xml" />
    </context>
  </spring>
  <appSettings>
    <!--<add key="owin:AutomaticAppStartup" value="false" />-->
    <add key="Spring.Data.NHibernate.Support.OpenSessionInViewModule.SessionFactoryObjectName" value="applicationSessionFactory" />
  </appSettings>
